<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple Computer//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>Label</key>
	<string>fr.raphaelyancey.pinwatcher</string>
	<key>ProgramArguments</key>
	<array>
		<string>sh</string>
		<string>-c</string>
		<string>php /Users/[username]/Library/Application\ Support/pinwatcher.php</string>
	</array>
	<key>StartInterval</key>
	<integer>3600</integer> <!-- 1h is default -->
	<key>StandardOutPath</key>
	<string>/Users/[username]/Library/Application\ Support/Pinwatcher/Pinwatcher.log</string>
</dict>
</plist>
